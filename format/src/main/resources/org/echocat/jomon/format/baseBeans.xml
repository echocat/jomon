<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="org.echocat.jomon.format.CombinedFormatProvider" class="org.echocat.jomon.format.CombinedFormatProvider" />
    <alias name="org.echocat.jomon.format.CombinedFormatProvider" alias="org.echocat.jomon.format.FormatProvider" />

    <bean id="org.echocat.jomon.format.CombinedFormatter" class="org.echocat.jomon.format.CombinedFormatter" />
    <alias name="org.echocat.jomon.format.CombinedFormatter" alias="org.echocat.jomon.format.Formatter" />

</beans>
